@model List<TravelHub.Models.Users>

@{
    ViewBag.Title = "Home Page";
}


<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">First</th>
        <th scope="col">Last</th>
        <th scope="col">Handle</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var a in Model)
    {
        <tr>
            <td>@a.UserId</td>
            <td>@a.FirstName</td>
            <td>@a.MiddleName</td>
            <td>@a.LastName</td>
        </tr>
    }

    </tbody>
</table>